{
  "name": "mtnmomo-ts",
  "version": "0.0.1",
  "description": "Unofficial Typescript Library for the MTN Mobile Money API",
  "keywords": [],
  "author": "Brian Ochan <brian.ochan@branchedcode.com> (https://branchedcode.com/)",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm,iife --minify --treeshake --no-splitting --shims --dts --clean --out-dir dist",
    "build:docs":"typedoc --options typedoc.json",
    "lint": "yarn tsc && yarn lint:ts && yarn prettier:check",
    "lint:fix": "yarn lint:ts --fix && yarn prettier:fix",
    "lint:ts": "eslint --ext '.ts,.tsx' . --cache",
    "test": "jest",
    "prettier:check": "prettier -c -u .",
    "prettier:fix": "prettier -c -u -w .",
    "commitlint": "sbin/commitlint.py"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.18.6",
    "@jest/globals": "29.4.1",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "axios": "1.2.6",
    "dotenv": "16.0.3",
    "eslint": "8.33.0",
    "eslint-config-typescript-shareable": "1.9.0",
    "eslint-plugin-functional": "5.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "29.4.1",
    "jest-environment-jsdom": "29.4.1",
    "prettier": "2.8.3",
    "ts-node": "10.9.1",
    "tsup": "^6.6.3",
    "typedoc": "^0.23.26",
    "typescript": "4.9.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/branchedcode/mtnmomo-ts.git"
  },
  "dependencies": {}
}

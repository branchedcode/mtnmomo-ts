{
  "$schema": "https://typedoc.org/schema.json", //autocompletion
  "entryPoints": ["./src/index.ts"],
  "out": "docs",
  "excludeNotDocumented": true,
  "excludeNotDocumentedKinds": ["Property", "Interface", "TypeAlias"],
  "exludePrivate": true,
  "excludeProtected": true,
  "includeVersion": true,
  "readme": "none",
  "revision": "main",
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  },
  "treatWarningsAsErrors": true,
  "requiredToBeDocumented": ["Enum", "Class"]
}
